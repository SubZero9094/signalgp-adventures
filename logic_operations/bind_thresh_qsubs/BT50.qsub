  PARAM__RND_ENV_STATE_TAGS=0

# 6:  1111-1140  => ES4  FS0  RNDSIG1
elif [ ${PBS_ARRAYID} -ge 1111 ] && [ ${PBS_ARRAYID} -le 1140 ]
then
  PARAM__ENVIRONMENT_STATES=4
  PARAM__ENVIRONMENT_FALSE_SIGNALS=0
  PARAM__RND_ENV_STATE_TAGS=1

# 7:  1141-1170  => ES4  FS4  RNDSIG0
elif [ ${PBS_ARRAYID} -ge 1141 ] && [ ${PBS_ARRAYID} -le 1170 ]
then
  PARAM__ENVIRONMENT_STATES=4
  PARAM__ENVIRONMENT_FALSE_SIGNALS=4
  PARAM__RND_ENV_STATE_TAGS=0

# 8:  1171-1200  => ES4  FS4  RNDSIG1
elif [ ${PBS_ARRAYID} -ge 1171 ] && [ ${PBS_ARRAYID} -le 1200 ]
then
  PARAM__ENVIRONMENT_STATES=4
  PARAM__ENVIRONMENT_FALSE_SIGNALS=4
  PARAM__RND_ENV_STATE_TAGS=1

